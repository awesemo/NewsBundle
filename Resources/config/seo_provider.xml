<?xml version="1.0" encoding="UTF-8" ?>
<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <parameters>
        <parameter key="rz.news.provider.seo.default.class">Rz\NewsBundle\Provider\Post\DefaultSeoProvider</parameter>
    </parameters>

    <services>
        <service id="rz.news.provider.seo.default" class="%rz.news.provider.seo.default.class%" shared="true">
            <tag name="rz.news.seo.provider" />
            <argument>rz.news.provider.seo.default</argument>
            <call method="setTranslator">
                <argument type="service" id="translator" />
            </call>
        </service>
    </services>
</container>
